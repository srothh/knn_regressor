import numpy as np

class kNNRegression:
    def __init__(self, neighbors):
        self.neighbors = neighbors
    
    def fit(self,X_train,y_train):
        self.X_train = X_train
        self.y_train = y_train
    
    def euclidiean_distance(self, X_train, X_test):
        for i in range(len(X_train)):
            distance = np.sqrt(np.sum((X_train[i]-X_test[i]))**2)
        return distance

    def predict(self, ):
        pass

    def accuracy(self, ):
        pass
